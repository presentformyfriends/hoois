{
    "master_picture": "app/assets/images/favicon.png",
    "favicon_design": {
        "ios": {
            "picture_aspect": "background_and_margin",
            "background_color": "#ff931e",
            "margin": "14%",
            "assets": {
                "ios6_and_prior_icons": true,
                "ios7_and_later_icons": true,
                "precomposed_icons": false,
                "declare_only_default_icon": true
            }
        },
        "desktop_browser": {
            "design": "background",
            "backgroundColor": "#ff931e",
            "backgroundRadius": 0.45,
            "imageScale": 0.8
        },
        "windows": {
            "picture_aspect": "white_silhouette",
            "background_color": "#da532c",
            "on_conflict": "override",
            "assets": {
                "windows_80_ie_10_tile": true,
                "windows_10_ie_11_edge_tiles": {
                    "small": true,
                    "medium": true,
                    "big": true,
                    "rectangle": true
                }
            }
        },
        "android_chrome": {
            "picture_aspect": "background_and_margin",
            "margin": "17%",
            "background_color": "#ff931e",
            "theme_color": "#ff931e",
            "manifest": {
                "name": "Hoois",
                "display": "standalone",
                "orientation": "not_set",
                "on_conflict": "override",
                "declared": true
            },
            "assets": {
                "legacy_icon": false,
                "low_resolution_icons": false
            }
        },
        "safari_pinned_tab": {
            "picture_aspect": "silhouette",
            "theme_color": "#ff931e"
        }
    },
    "settings": {
        "scaling_algorithm": "Mitchell",
        "error_on_image_too_small": false,
        "readme_file": false,
        "html_code_file": false,
        "use_path_as_is": false
    }
}

